@model IEnumerable<Lab01.Models.Product>

<table class="table">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Name</th>
			<th scope="col">Price</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<th scope="row">@item.Id</th>
				<td>@item.Name</td>
				<td>@item.Price</td>
			</tr>
		}
	</tbody>
</table>
